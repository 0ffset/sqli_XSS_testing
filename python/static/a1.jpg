							<img
								data-src="https://{{image.image.defaultHost}}/i/{{image.image.endpoint}}/{{image.image.name}}?$mobileFeaturedCategories$"
								class="img-responsive featuredbanner_image deferimage"
								alt="{{image.imageAltText}}"
