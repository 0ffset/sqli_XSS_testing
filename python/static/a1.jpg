<script id="template-herobanner" type="text/x-handlebars-template">
  <style>
    {{#if mobileTextColour}}
      @media only screen and (max-width: 639px){
        .hero-component__content .amplience__text-color--herobanner {
          color: {{mobileTextColour}};
        }
      }
    {{/if}}
    {{#if desktopTextColour}}
      @media only screen and (min-width: 640px){
        .hero-component__content .amplience__text-color--herobanner {
          color: {{desktopTextColour}};
        }
      }
    {{/if}}
  </style>
  <div class="disp-img simple-banner">
    <div class="hero-component">
        <div class="hero-component__image">
         {{#if (is_status  bannerBackground.mediaType 'video')}} 
         <a href={{imageCTA}}>
          <video autoplay loop class="js-video" poster="https://{{bannerBackground.defaultHost}}/v/{{bannerBackground.endpoint}}/{{bannerBackground.name}}.jpg">
            <source src="https://{{bannerBackground.defaultHost}}/v/{{bannerBackground.endpoint}}/{{bannerBackground.name}}/mp4_720p">           
          </video>
          </a>
           
        {{else}} 
          <a href={{imageCTA}}>
            <picture> 
              <source srcset="https://{{bannerBackground.defaultHost}}/i/{{bannerBackground.endpoint}}/{{bannerBackground.name}}?$desktopHeroBanner$&v=123789&fmt=webp" type="image/webp" media= "(min-width: 769px)" >             
              <source srcset="https://{{bannerBackground.defaultHost}}/i/{{bannerBackground.endpoint}}/{{bannerBackground.name}}?$desktopHeroBanner$&v=123789" media= "(min-width: 769px)" >             
			  <img class="img-responsive" src="https://{{bannerBackground.defaultHost}}/i/{{bannerBackground.endpoint}}/{{bannerBackground.name}}?$mobileHeroBanner$&v=123789">
 			 </picture>
            </a>
          {{/if}}          
        </div>

        {{#if textPosition}}
        {{#if (is_status textPosition 'Left')}}
           <div class="hero-component__content hero-component__content-left js-content-link visible-lg visible-md visible-sm">
        {{/if}}
        {{#if (is_status textPosition 'Right')}}
             <div class=" hero-component__content js-content-link hero-component__content-right visible-lg visible-md visible-sm">
        {{/if}}
         {{#if (is_status textPosition 'Center')}}
            <div class="hero-component__content js-content-link hero-component__content-center visible-lg visible-md visible-sm">
        {{/if}}
        {{else}}
             <div class="hero-component__content js-content-link visible-lg visible-md visible-sm">
        {{/if}}
          {{#if textAlignment}}
          {{#if (is_status textAlignment 'Left')}}
               <div class="hero-component-text--left">
          {{/if}}
          {{#if (is_status textAlignment 'Right')}}
               <div class="hero-component-text--right">
          {{/if}}
           {{#if (is_status textAlignment 'Center')}}
               <div class="hero-component-text--center">
          {{/if}}
          {{else}}
               <div class="hero-component-text--left">
         {{/if}}
               {{#if promotionMiddleLargeText}}

                    {{#if promotionMiddleLargeTextTextSize}}
                      {{#if (is_status  promotionMiddleLargeTextTextSize 'Large')}}
                        <h1 class="hero-component__heading hero-component__promotionalheading hero-component__promotionalheading--large js-promotional-heading amplience__text-color--herobanner">{{promotionMiddleLargeText}}</h1>
                        {{else}}
                        <h1 class="hero-component__heading  hero-component__promotionalheading js-promotional-heading amplience__text-color--herobanner">{{promotionMiddleLargeText}}</h1>
                      {{/if}}

                    {{else}}
                        <h1 class="hero-component__heading hero-component__promotionalheading hero-component__promotionalheading js-promotional-heading amplience__text-color--herobanner">{{promotionMiddleLargeText}}</h1>
                    {{/if}} 
                      <p class="hero-component__lead hero-component__promotionallead amplience__text-color--herobanner">{{promotionBottomSmallText}}</p>
                  {{else}}
                    <h1 class="hero-component__heading amplience__text-color--herobanner">{{middleLargeText}}</h1>
                    <p class="hero-component__lead amplience__text-color--herobanner">{{bottomSmallText}}</p>
              {{/if}} 
               
               


            <div class="hero-component__action">
            {{#if cta.url}}
            <a class="btn btn-primary btn-block-xs js-shop-now" href="{{cta.url}}">{{cta.label}}</a>
            {{/if}}
          </div>
        </div>
       </div>

        {{#if mobileTextPosition}}
        {{#if (is_status mobileTextPosition 'Left')}}
           <div class="hero-component__content hero-component__content-left js-content-link visible-xs">
        {{/if}}
        {{#if (is_status mobileTextPosition 'Right')}}
             <div class=" hero-component__content js-content-link hero-component__content-right visible-xs">
        {{/if}}
         {{#if (is_status mobileTextPosition 'Center')}}
            <div class="hero-component__content js-content-link hero-component__content-center visible-xs">
        {{/if}}
        {{else}}
             <div class="hero-component__content js-content-link visible-xs">
        {{/if}}
               {{#if mobileTextAlignment}}
                  {{#if (is_status mobileTextAlignment 'Left')}}
                       <div class="hero-component-text--left visible-xs">
                  {{/if}}
                  {{#if (is_status mobileTextAlignment 'Right')}}
                       <div class="hero-component-text--right visible-xs">
                  {{/if}}
                   {{#if (is_status mobileTextAlignment 'Center')}}
                       <div class="hero-component-text--center visible-xs">
                  {{/if}}
              {{else}}
                   <div class="hero-component-text--left visible-xs">
             {{/if}}

            {{#if promotionMiddleLargeText}}
                <h1 class="hero-component__heading hero-component__promotionalheading js-promotional-heading amplience__text-color--herobanner">{{promotionMiddleLargeText}}</h1>
                  <p class="hero-component__lead hero-component__promotionallead amplience__text-color--herobanner">{{promotionBottomSmallText}}</p>
              {{else}}
                <h1 class="hero-component__heading amplience__text-color--herobanner">{{middleLargeText}}</h1>
                <p class="hero-component__lead amplience__text-color--herobanner">{{bottomSmallText}}</p>
            {{/if}} 


            <div class="hero-component__action">
            {{#if cta.url}}
            <a class="btn btn-primary btn-block-xs js-shop-now" href="{{cta.url}}">{{cta.label}}</a>
            {{/if}}
          </div>
        </div>
       </div>

    </div>    
  </div>
</script>



